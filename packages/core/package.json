{
  "name": "@arktype-form-builder/core",
  "private": false,
  "version": "0.1.0",
  "description": "Automated form builder with Arktype",
  "repository": "https://github.com/IzumiSy/arktype-form-builder",
  "homepage": "https://github.com/IzumiSy/arktype-form-builder",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development tsup --watch",
    "build": "tsup",
    "type-check": "tsc --noEmit --incremental --pretty",
    "package:publish": "pnpm build && pnpm publish --access public"
  },
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "keywords": [
    "arktype",
    "form",
    "forms",
    "autoform",
    "react",
    "validation",
    "schema-driven"
  ],
  "license": "MIT",
  "peerDependencies": {
    "arktype": "^2",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@swc/core": "^1.10.8",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "react-hook-form": "^7.54.2"
  }
}
